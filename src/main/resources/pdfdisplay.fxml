<?xml version="1.0" encoding="UTF-8"?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.text.Text?>
<AnchorPane xmlns="http://javafx.com/javafx"
            xmlns:fx="http://javafx.com/fxml"
            fx:controller="controller.PDFdisplayController"
            prefHeight="1000.0" prefWidth="1600.0">
    <ImageView preserveRatio="true" layoutY="50" layoutX="300" fitHeight="900" fx:id="PDFimage"/>
    <Button fx:id="NextButton" onAction="#nextButton" layoutX="50" layoutY="100" prefWidth="100">Volgende pagina</Button>
    <Button fx:id="PreviousButton" onAction="#previousButton" layoutX="50" layoutY="200" prefWidth="100">Vorige pagina</Button>
    <Text fx:id="PageNumber" layoutX="50" layoutY="300">Pagina: </Text>

</AnchorPane>
